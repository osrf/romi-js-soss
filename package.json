  },
  "devDependencies": {
    "@types/jasmine": "^3.4.0",
    "@types/karma": "^3.0.7",
    "@types/node": "^12.7.2",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
    "jasmine": "^3.4.0",
    "jasmine-ts": "^0.3.0",
    "karma": "^4.3.0",
