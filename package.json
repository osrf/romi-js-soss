{
  "name": "@osrf/romi-js-soss-transport",
  "version": "0.0.2-alpha.1",
  "description": "soss transport for romi",
  "scripts": {
    "build": "webpack && tsc",
    "build:prod": "webpack --mode production && tsc",
    "prepare": "npm run build:prod",
    "watch": "webpack --watch",
    "clean": "tsc --build --clean && rm -rf bundle",
    "test": "ts-node -O '{\"module\": \"commonjs\"}' node_modules/.bin/jasmine",
    "lint": "eslint . --ext .ts,.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/osrf/romi-js-soss-transport"
  },
  "author": "Teo Koon Peng <koonpeng@openrobotics.org>",
  "license": "Apache-2.0",
  "dependencies": {
    "@osrf/romi-js-core-interfaces": "git+https://github.com/osrf/romi-js-core-interfaces.git#develop",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@types/jasmine": "^3.4.0",
    "@types/karma": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "eslint": "^6.8.0",
    "jasmine": "^3.4.0",
    "karma": "^4.3.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^3.1.1",
    "rclnodejs": "^0.13.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.5.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.9"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
